import sqlite3
conn = sqlite3.connect("hospital.db")
cur = conn.cursor()

cur.execute("""CREATE TABLE IF NOT EXISTS patients (
            id INTEGER PRIMARY KEY AUTOINCREMENT,
            doctorID INTEGER,
            name TEXT NOT NULL,
            gender TEXT NOT NULL,
            injury TEXT NOT NULL,
            phone TEXT
            )
            """)

conn.commit()
print("Table done")
conn.close()
